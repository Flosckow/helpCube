{
  "parser": "babel-eslint",
  "extends": [
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "eol-last": 0,
    "prefer-const": 0,
    "max-len": ["error", {"code":  120}],
    "prettier/prettier": ["error"]
  }
}